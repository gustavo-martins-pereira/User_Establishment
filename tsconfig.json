{
    "extends": "@tsconfig/node22/tsconfig.json",
    "compilerOptions": {
        "outDir": "dist",
        "baseUrl": "./src",
        "paths": {
            "@aws/*": ["aws/*"],
            "@configs/*": ["configs/*"],
        },
        "allowImportingTsExtensions": true,
        "noEmit": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"],
}